{
  "name": "TimeEater",
  "description": "A Vue.js web extension",
  "version": "1.0",
  "manifest_version": 2,
  "icons": {
    "48": "icons/icon_48.png",
    "128": "icons/icon_128.png"
  },
  "permissions": [
    "storage",
    "tabs",
    "activeTab"
  ],
  "browser_action": {
    "default_title": "TimeEater",
    "default_popup": "popup/popup.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["content/content.js"]
    }
  ]
}
